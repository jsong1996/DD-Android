<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    tools:context=".ArtistTab.MusicFragment">


    <ImageButton
        android:layout_width="75dip"
        android:layout_height="75dip"
        android:id = "@+id/tidal_button"
        android:background="@drawable/tidal_icon"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="85dip"
        android:layout_height="85dip"
        android:id = "@+id/soundcloud_button"
        android:background="@drawable/soundcloud_icon"
        android:layout_alignBottom="@+id/spotify_button"
        android:layout_toRightOf="@+id/youtube_button"
        android:layout_toEndOf="@+id/youtube_button" />

    <ImageButton
        android:layout_width="75dip"
        android:layout_height="75dip"
        android:id="@+id/itunes_button"
        android:background="@drawable/itunes_icon"
        android:layout_above="@+id/spotify_button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="70dip"
        android:layout_height="70dip"
        android:id="@+id/youtube_button"
        android:background="@drawable/youtube_icon"
        android:layout_above="@+id/spotify_button"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="75dip"
        android:layout_height="75dip"
        android:id="@+id/spotify_button"
        android:background="@drawable/spotify_icon"
        android:layout_below="@+id/tidal_button"
        android:layout_toRightOf="@+id/tidal_button"
        android:layout_toEndOf="@+id/tidal_button" />


</RelativeLayout>
